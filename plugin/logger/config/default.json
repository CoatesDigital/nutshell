{
	"config":
	{
		"writers": 
		{
			"rootLog": 
			{
				"class":			"DailyFileWriter",
				"output":			"{APP_HOME}/log/root-{DATE}.log"
			},
			"dbLog":
			{
				"class":			"DbWriter",
				"connector":		"mysql",
				"table":			"log"
			}
		},
		"loggers": 
		{
			"__ROOT__": 
			{
				"writers":
				[
					{
						"writer":	"rootLog",
						"level" :	"WARN" 
					}
				]
			}
		}
	}
}