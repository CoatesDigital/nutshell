{
	"config":
	{
		"writers" : 
		{
			"rootLog" : 
			{
				"type" : 
				{
					"class" : "FileWriter",
					"output" : "{NS_HOME}/log/root.log"
				}
				"message" : "%d [%P] - %m%n"
			}
		},
		"loggers": 
		{
			"__ROOT__" : 
			[
				{
					"writer" : "rootLog",
					"level" : "WARN" 
				}
			]
		}
	}
}