{
	"config":
	{
		"writers": 
		{
			"rootLog": 
			{
				"class":			"DailyFileWriter",
				"output":			"{NS_HOME}/log/root-{DATE}.log",
				"messagePattern":	"%d [%P] - %m%n"
			},
			"dbLog":
			{
				"class":			"DailyFileWriter",
				"connector":		"mysql",
				"table":			"log",
				"messagePattern":	"%d [%P] - %m%n"
			}
		},
		"loggers": 
		{
			"__ROOT__": 
			[
				{
					"writer":	"rootLog",
					"level" :	"WARN" 
				}
			]
		}
	}
}